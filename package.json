{
    "name": "react-cool-input",
    "version": "0.0.1",
    "description": "A cool React input component for very specific use case like OTP, 2FA code, etc.",
    "main": "dist/plugin.js",
    "module": "dist/plugin.es.js",
    "typings": "dist/index.d.ts",
    "unpkg": "dist/plugin.umd.js",
    "files": [
        "dist"
    ],
    "license": "MIT",
    "scripts": {
        "build": "tsc -p tsconfig.base.json && rollup -c && rm -rf compiled",
        "clean": "rm -rf dist",
        "dev": "cross-env NODE_ENV=development webpack-dev-server",
        "format": "prettier --config .prettierrc.json './src/**/*.ts' --write",
        "lint": "eslint --config .eslintrc.json --ext .ts ./src",
        "prebuild": "rm -rf dist",
        "start": "serve dist",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "author": {
        "name": "Shikhar Sharma",
        "email": "ceoshikhar@gmail.com",
        "url": "https://ceoshikhar.com"
    },
